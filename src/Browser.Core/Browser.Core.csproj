  </PropertyGroup>

  <ItemGroup>
    <None Update="App_Data\script\EmptyScript.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="App_Data\html\.gitkeep">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="App_Data\linux\phantomjs">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="App_Data\osx\phantomjs">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
